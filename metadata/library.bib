	isbn = {0000000000000},
	issn = {2474-7882},
	journal = {Environmental Epidemiology},
	keywords = {birth weight,fetal growth,metals,mixtures,prenatal exposure},
	number = {2},
	pages = {e075},
	title = {{Urinary trace metals in association with fetal ultrasound measures during pregnancy}},
	volume = {4},
