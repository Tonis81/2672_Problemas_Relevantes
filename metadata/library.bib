	keywords = {Cohort Studies,Data Interpretation,Health Services Research,Health Services Research: methods,Hospital Mortality,Humans,Logistic Models,Odds Ratio,Outcome Assessment (Health Care),Outcome Assessment (Health Care): methods,Risk,Statistical},
	number = {19},
