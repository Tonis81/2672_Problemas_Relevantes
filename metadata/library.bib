	doi = {10.1371/journal.pone.0080691},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Melkonian et al. - 2013 - Urinary and dietary analysis of 18,470 Bangladeshis reveal a correlation of rice consumption with arsenic expo.pdf:pdf},
	issn = {19326203},
	journal = {PLoS ONE},
	number = {11},
