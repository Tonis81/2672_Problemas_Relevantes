	title = {{Levels of urinary total and speciated arsenic in the US population: National Health and Nutrition Examination Survey 2003-2004}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/18523458{\%}5Cnhttp://www.nature.com/jes/journal/v19/n1/pdf/jes200832a.pdf},
	volume = {19},
