	journal = {Journal of preventive medicine and public health = Yebang {\u U}ihakhoe chi},
	keywords = {arsenic,carcinogenicity,health effects},
	number = {5},
	pages = {245--52},
	pmid = {25284195},
	title = {{Health effects of chronic arsenic exposure.}},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4186552{\&}tool=pmcentrez{\&}rendertype=abstract},
	volume = {47},
	year = {2014},
	bdsk-url-1 = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4186552%7B%5C&%7Dtool=pmcentrez%7B%5C&%7Drendertype=abstract},
