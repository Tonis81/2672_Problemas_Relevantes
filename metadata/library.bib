	issn = {00489697},
	journal = {Science of The Total Environment},
	keywords = {Arsenic speciation,Dietary exposure,Food,Human health,Inorganic arsenic,Risk assessment},
	pages = {1228--1239},
	publisher = {Elsevier B.V.},
	title = {{Human exposure to dietary inorganic arsenic and other arsenic species: State of knowledge, gaps and uncertainties}},
