@article{Greenland1986,
	abstract = {Non-identifiability of parameters is a well-recognized problem in classical statistics, and Bayesian statisticians have long recognized the importance of exchangeability assumptions in making statistical inferences. A seemingly unrelated problem in epidemiology is that of confounding: bias in estimation of the effects of an exposure on disease risk, due to inherent differences in risk between exposed and unexposed individuals. Using a simple deterministic model for exposure effects, a logical connection is drawn between the concepts of identifiability, exchangeability, and confounding. This connection allows one to view the problem of confounding as arising from problems of identifiability, and reveals the exchangeability assumptions that are implicit in confounder control methods. It also provides further justification for confounder definitions based on comparability of exposure groups, as opposed to collapsibility-based definitions.},
	author = {Greenland, Sander and Robins, James M.},
	doi = {10.1093/ije/15.3.413},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Greenland, Robins - 1986 - Identifiability, exchangeability, and epidemiological confounding.pdf:pdf},
	isbn = {0300-5771 (Print)},
	issn = {03005771},
	journal = {International Journal of Epidemiology},
	number = {3},
	pages = {413--419},
	pmid = {3771081},
	title = {{Identifiability, exchangeability, and epidemiological confounding}},
	volume = {15},
	year = {1986},
	bdsk-url-1 = {https://doi.org/10.1093/ije/15.3.413}}

@misc{Wright2010,
	abstract = {Exposure to environmental toxins during critical periods of prenatal and/or postnatal development may alter the normal course of lung morphogenesis and maturation, potentially resulting in changes that affect both structure and function of the respiratory system. Moreover, these early effects may persist into adult life magnifying the potential public health impact. Aberrant or excessive pro-inflammatory immune responses, occurring both locally and systemically, that result in inflammatory damage to the airway are a central determinant of lung structure-function changes throughout life. Disruption of neuroendocrine function in early development, specifically the hypothalamic-pituitary-adrenal (HPA) axis, may alter functional status of the immune system. Autonomic nervous system (ANS) function (sympathovagal imbalance) is another integral component of airway function and immunity in childhood. This overview discusses the evidence linking psychological factors to alterations in these interrelated physiological processes that may, in turn, influence childhood lung function and identifies gaps in our understanding. {\textcopyright} 2010 Elsevier B.V.},
	author = {Wright, Rosalind J.},
	booktitle = {Biological Psychology},
	doi = {10.1016/j.biopsycho.2010.01.007},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Wright - 2010 - Perinatal stress and early life programming of lung structure and function.pdf:pdf},
	issn = {03010511},
	keywords = {Airway hyperresponsiveness,Airway inflammation,Autonomic nervous system,Hypothalamic-pituitary-adrenal axis,Immunomodulation,Lung function,Maternal prenatal stress,Postnatal stress,Programming},
	month = {apr},
	number = {1},
	pages = {46--56},
	publisher = {Biol Psychol},
	title = {{Perinatal stress and early life programming of lung structure and function}},
	url = {https://pubmed.ncbi.nlm.nih.gov/20080145/},
	volume = {84},
	year = {2010},
	bdsk-url-1 = {https://pubmed.ncbi.nlm.nih.gov/20080145/},
