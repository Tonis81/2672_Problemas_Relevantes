	eprint = {NIHMS150003},
	isbn = {1055-9965},
	issn = {10559965},
	journal = {Cancer Epidemiology Biomarkers and Prevention},
	number = {7},
	pages = {1187--1194},
	pmid = {24793955},
	title = {{The global burden of disease for skin, lung, and bladder cancer caused by arsenic in food}},
	volume = {23},
	year = {2014},
