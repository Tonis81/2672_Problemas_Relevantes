	issn = {18736351},
	journal = {Food and Chemical Toxicology},
	keywords = {Apple juice,Exposure assessment,Metals,Risk assessment},
	pages = {42--50},
	publisher = {Elsevier Ltd},
	title = {{Screening level health risk assessment of selected metals in apple juice sold in the United States}},
