@article{Aschner2005,
	abstract = {Manganese (Mn) is an essential mineral. It is present in virtually all diets at low concentrations. The principal route of intake for Mn is via food consumption, but in occupational cohorts, inhalation exposure may also occur (this subject will not be dealt with in this review). Humans maintain stable tissue levels of Mn. This is achieved via tight homeostatic control of both absorption and excretion. Nevertheless, it is well established that exposure to high oral, parenteral or ambient air concentrations of Mn can result in elevations in tissue Mn levels. Excessive Mn accumulation in the central nervous system (CNS) is an established clinical entity, referred to as manganism. It resembles idiopathic Parkinson's disease (IPD) in its clinical features, resulting in adverse neurological effects both in laboratory animals and humans. This review focuses on an area that to date has received little consideration, namely the potential exposure of parenterally fed neonates to exceedingly high Mn concentrations in parenteral nutrition solutions, potentially increasing their risk for Mn-induced adverse health sequelae. The review will consider (1) the essentiality of Mn; (2) the concentration ranges, means and variation of Mn in various foods and infant formulas; (3) the absorption, distribution, and elimination of Mn after oral exposure and (4) the factors that raise a theoretical concern that neonates receiving total parenteral nutrition (TPN) are exposed to excessive dietary Mn. {\textcopyright} 2005 Elsevier Ltd. All rights reserved.},
	author = {Aschner, Judy L. and Aschner, Michael},
	doi = {10.1016/j.mam.2005.07.003},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Aschner, Aschner - 2005 - Nutritional aspects of manganese homeostasis.pdf:pdf},
	isbn = {0098-2997 (Print)$\backslash$r0098-2997 (Linking)},
	issn = {00982997},
	journal = {Molecular Aspects of Medicine},
	keywords = {Central nervous system,Formula,Manganese,Total parenteral nutrition},
	pages = {353--362},
	pmid = {16099026},
	title = {{Nutritional aspects of manganese homeostasis}},
	volume = {26},
	year = {2005},
	bdsk-url-1 = {https://doi.org/10.1016/j.mam.2005.07.003}}

@article{Banerjee2013,
	abstract = {Arsenic in drinking water may cause major deleterious health impacts including death. Although arsenic in rice has recently been demonstrated to be a potential exposure route for humans, there has been to date no direct evidence for the impact of such exposure on human health. Here we show for the first time, through a cohort study in West Bengal, India, involving over 400 human subjects not otherwise significantly exposed to arsenic through drinking water, elevated genotoxic effects, as measured by micronuclei (MN) in urothelial cells, associated with the staple consumption of cooked rice with {\textgreater}200 $\mu$g/kg arsenic. Further work is required to determine the applicability to populations with different dietary and genetic characteristics, but with over 3 billion people in the world consuming rice as a staple food and several percent of this rice containing such elevated arsenic concentrations, this study raises considerable concerns over the threat to human health.},
