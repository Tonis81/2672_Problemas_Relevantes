	issn = {20726643},
	journal = {Nutrients},
	keywords = {Arsenic exposure,Bangladesh,Dietary assessment,Food frequency questionnaire,Pregnancy},
	number = {4},
	title = {{Associations between diet and toenail arsenic concentration among pregnant women in bangladesh: A prospective study}},
	volume = {9},
