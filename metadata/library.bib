@article{Wheeler2021,
	abstract = {There has been a growing interest in the literature on multiple environmental risk factors for diseases and an increasing emphasis on assessing multiple environmental exposures simultaneously in epidemiologic studies of cancer. One method used to analyze exposure to multiple chemical exposures is weighted quantile sum (WQS) regression. While WQS regression has been demonstrated to have good sensitivity and specificity when identifying important exposures, it has limitations including a two-step model fitting process that decreases power and model stability and a requirement that all exposures in the weighted index have associations in the same direction with the outcome, which is not realistic when chemicals in different classes have different directions and magnitude of association with a health outcome. Grouped WQS (GWQS) was proposed to allow for multiple groups of chemicals in the model where different magnitude and direction of associations are possible for each group. However, GWQS shares the limitation of WQS of a two-step estimation process and splitting of data into training and validation sets. In this paper, we propose a Bayesian group index model to avoid the estimation limitation of GWQS while having multiple exposure indices in the model. To evaluate the performance of the Bayesian group index model, we conducted a simulation study with several different exposure scenarios. We also applied the Bayesian group index method to analyze childhood leukemia risk in the California Childhood Leukemia Study (CCLS). The results showed that the Bayesian group index model had slightly better power for exposure effects and specificity and sensitivity in identifying important chemical exposure components compared with the existing frequentist method, particularly for small sample sizes. In the application to the CCLS, we found a significant negative association for insecticides, with the most important chemical being carbaryl. In addition, for children who were born and raised in the home where dust samples were taken, there was a significant positive association for herbicides with dacthal being the most important exposure. In conclusion, our approach of the Bayesian group index model appears able to make a substantial contribution to the field of environmental epidemiology.},
	author = {Wheeler, David C. and Rustom, Salem and Carli, Matthew and Whitehead, Todd P. and Ward, Mary H. and Metayer, Catherine},
	doi = {10.3390/ijerph18073486},
	file = {:Users/tonisignes/Dropbox (Personal)/TONI SIGNES/A{\_}BOOKS {\&} ARTICLES/ARTICLES MENDELEY pdf/Wheeler2021.pdf:pdf},
	issn = {16604601},
	journal = {International Journal of Environmental Research and Public Health},
	keywords = {Cancer,Chemical mixtures,Environment,Mixture analysis},
	number = {7},
	pmid = {33801661},
	title = {{Bayesian group index regression for modeling chemical mixtures and cancer risk}},
	volume = {18},
	year = {2021},
	bdsk-url-1 = {https://doi.org/10.3390/ijerph18073486}}

@article{Mwangome2014,
	abstract = {The World Health Organisation (WHO) recommends weight-for-length/height (WFL/H), represented as a Z score for diagnosing acute malnutrition among children aged 0 to 60 months. Under controlled conditions, weight, height and length measurements have high degree of reliability. However, the reliability when combined into a WFL/H Z score, in all settings is unclear. We conducted a systematic review of published studies assessing the reliability of WFL/Hz on PubMed and Google scholar. Studies were included if they presented reliability scores for the derived index of WFL/Hz, for children under 5 years. Meta-analysis was conducted for a pooled estimate of reliability overall, and for children above and below 24 months old. Twenty six studies on reliability of anthropometry were identified but only three, all community-based studies, reported reliability scores for WFL/Hz. The overall pooled intra-class correlation coefficient (ICC) estimate for WFL/Hz among children aged 0 to 60 months was 0.81 (95{\%} CI 0.64 to 0.99). Among children aged less than 24 months the pooled ICC estimate from two studies was 0.72 (95{\%} CI 0.67 to 0.77) while the estimate reported for children above 24 months from one study was 0.97 (95{\%} CI 0.97 to 0.99). Although WFL/Hz is recommended for diagnosis of acute under nutrition among children below 5 years, information on its reliability in all settings is sparse. In community settings, reliability of WFL/Hz is considerably lower than for absolute measures of weight and length/height, especially in younger children. The reliability of WFL/Hz needs further evaluation.},
	author = {Mwangome, Martha K. and Berkley, James A.},
	doi = {10.1111/mcn.12124},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Mwangome, Berkley - 2014 - The reliability of weight-for-lengthheight Z scores in children.pdf:pdf},
	isbn = {1740-8709},
	issn = {17408709},
	journal = {Maternal and Child Nutrition},
	keywords = {Children,Reliability,Weight-for-height,Weight-for-length},
	number = {4},
	pages = {474--480},
	pmid = {24785183},
	title = {{The reliability of weight-for-length/height Z scores in children}},
	volume = {10},
	year = {2014},
	bdsk-url-1 = {https://doi.org/10.1111/mcn.12124}}

@article{Farzan2016e,
	abstract = {BACKGROUND Gestational diabetes mellitus (GDM) is a major pregnancy complication with detrimental effects for both mothers and their children. Accumulating evidence has suggested a potential role for arsenic (As) exposure in the development of GDM, but current studies have not assessed As exposure from water, urine or toenail samples. METHODS We investigated the association between As exposure and risk of glucose intolerance and GDM among 1151 women enrolled in the New Hampshire Birth Cohort Study. Arsenic was measured in home well water and via biomarkers (i.e., maternal urine collected {\~{}}24-28 weeks gestation and toenail clippings collected 2 weeks postpartum). RESULTS A total of 105 (9.1 {\%}) of women were diagnosed with glucose intolerance and 14 (1.2 {\%}) of women were diagnosed with GDM. A total of 10.3 {\%} of women had water As levels above 10 $\mu$g/L, with a mean As level of 4.2. Each 5 $\mu$g/L increase in As concentration in home well water was associated with a {\~{}}10 {\%} increased odds of GDM (OR: 1.1, 95 {\%} CI 1.0, 1.2). A positive and statistically significant association also was observed between toenail As and GDM (OR: 4.5, 95 {\%} CI 1.2, 16.6), but not urinary arsenic (OR: 0.8, 95 {\%} CI 0.3, 2.4). In a stratified analysis, the association between water As and GDM and glucose intolerance was largely limited to obese women (OR: 1.7, 95 {\%} CI 1.0, 2.8). CONCLUSIONS Our findings support the role of As exposure via water from private wells in the incidence of GDM and that this association may be modified by body composition.},
	author = {Farzan, Shohreh F. and Gossai, Anala and Chen, Yu and Chasan-Taber, Lisa and Baker, Emily and Karagas, M. R.},
	doi = {10.1186/s12940-016-0194-0},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Farzan et al. - 2016 - Maternal arsenic exposure and gestational diabetes and glucose intolerance in the New Hampshire birth cohort stud.pdf:pdf;:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Farzan et al. - 2016 - Maternal arsenic exposure and gestational diabetes and glucose intolerance in the New Hampshire birth cohort s(2).pdf:pdf},
	issn = {1476069X},
	journal = {Environmental Health: A Global Access Science Source},
	keywords = {Arsenic,Gestational diabetes,Glucose intolerance,New Hampshire,Pregnancy cohort},
	number = {1},
	pages = {1--8},
	pmid = {27825389},
	publisher = {Environmental Health},
	title = {{Maternal arsenic exposure and gestational diabetes and glucose intolerance in the New Hampshire birth cohort study}},
	url = {http://dx.doi.org/10.1186/s12940-016-0194-0},
	volume = {15},
	year = {2016},
	bdsk-url-1 = {http://dx.doi.org/10.1186/s12940-016-0194-0}}

