	issn = {1096-0333},
	journal = {Toxicology and applied pharmacology},
	keywords = {Arsenic,Arsenic: metabolism,Arsenic: toxicity,Arsenic: urine,Bangladesh,Child Development,Child Development: drug effects,Environmental Exposure,Environmental Exposure: adverse effects,Environmental Exposure: analysis,Environmental Pollutants,Environmental Pollutants: metabolism,Environmental Pollutants: toxicity,Environmental Pollutants: urine,Female,Humans,Infant,Longitudinal Studies,Male,Multivariate Analysis,Regression Analysis,Weaning},
	month = {sep},
