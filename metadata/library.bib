@article{Wasserman2014,
	abstract = {BACKGROUND: In recent studies in Bangladesh and elsewhere, exposure to arsenic (As) via drinking water is negatively associated with performance-related aspects of child intelligence (e.g., Perceptual Reasoning, Working Memory) after adjustment for social factors. Because findings are not easily generalizable to the US, we examine this relation in a US population. METHODS: In 272 children in grades 3-5 from three Maine school districts, we examine associations between drinking water As (WAs) and intelligence (WISC-IV). RESULTS: On average, children had resided in their current home for 7.3 years (approximately 75{\%} of their lives). In unadjusted analyses, household well WAs is associated with decreased scores on most WISC-IV Indices. With adjustment for maternal IQ and education, HOME environment, school district and number of siblings, WAs remains significantly negatively associated with Full Scale IQ and Perceptual Reasoning, Working Memory and Verbal Comprehension scores. Compared to those with WAs {\textless} 5 mug/L, exposure to WAs {\textgreater}/= 5 mug/L was associated with reductions of approximately 5-6 points in both Full Scale IQ (p {\textless} 0.01) and most Index scores (Perceptual Reasoning, Working Memory, Verbal Comprehension, all p's {\textless} 0.05). Both maternal IQ and education were associated with lower levels of WAs, possibly reflecting behaviors (e.g., water filters, residential choice) limiting exposure. Both WAs and maternal measures were associated with school district. CONCLUSIONS: The magnitude of the association between WAs and child IQ raises the possibility that levels of WAs {\textgreater}/= 5 mug/L, levels that are not uncommon in the United States, pose a threat to child development.},
	author = {Wasserman, Gail A. and Liu, Xinhua and LoIacono, Nancy J. and Kline, Jennie and Factor-Litvak, Pam and {Van Geen}, Alexander and Mey, Jacob L. and Levy, Diane and Abramson, Richard and Schwartz, Amy and Graziano, Joseph H.},
	doi = {10.1186/1476-069X-13-23},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Wasserman et al. - 2014 - A cross-sectional study of well water arsenic and child IQ in Maine schoolchildren.pdf:pdf;:Users/tonisignes/Desktop/Dropbox/TONI SIGNES/A{\_}BOOKS {\&} ARTICLES/ARTICLES MENDELEY pdf/A cross-sectional study of well water arsenic and child IQ in Maine schoolchildren.pdf:pdf},
	isbn = {1476-069X (Electronic)$\backslash$r1476-069X (Linking)},
	issn = {1476069X},
	journal = {Environmental Health: A Global Access Science Source},
	keywords = {Arsenic,Child intelligence,WISC-IV,Well water,Working Memory},
