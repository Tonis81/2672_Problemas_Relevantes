	isbn = {3497775932},
	issn = {2305-6304},
	journal = {Toxics},
	keywords = {environmental exposure,finger-tapping,heavy metals,human hair,impulsivity,n-back,testosterone,working memory},
	number = {1},
	pages = {1--16},
	title = {{Levels of Metals in Hair in Childhood: Preliminary Associations with Neuropsychological Behaviors}},
	url = {http://www.mdpi.com/2305-6304/2/1/1/},
	volume = {2},
	year = {2013},
