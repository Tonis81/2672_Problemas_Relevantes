	journal = {Food science and technology international = Ciencia y tecnologia de los alimentos internacional},
	keywords = {Adolescent,Arsenic,Child,Chile,Environmental Exposure,Environmental Pollutants,Female,Food Analysis,Food Contamination,Humans,Male,Mercury,Schools,administration {\&} dosage,analysis,chemistry},
