	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Mao et al. - 2019 - Human health risks of heavy metals in paddy rice based on transfer characteristics of heavy metals from soil to rice.pdf:pdf},
	issn = {0341-8162},
	journal = {CATENA},
	keywords = {Health risk assessment,Heavy metals,Paddy rice,Transfer factor,Yangtze River Delta},
	month = {apr},
