	keywords = {Cancer risk,Inorganic arsenic,Non-carcinogenic risk,Seafood,Total arsenic},
	number = {January},
	pages = {322--331},
	pmid = {30654099},
	title = {{Systematic review of arsenic in fresh seafood from the Mediterranean Sea and European Atlantic coasts: A health risk assessment}},
