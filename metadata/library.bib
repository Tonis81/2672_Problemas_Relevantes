	issn = {18733492},
	journal = {Clinica Chimica Acta},
	keywords = {Breast cancer metallomics,ICP-MS,Noninvasive breast cancer screening,Urinary copper,Urinary lead,Urinary metallomics},
	month = {jan},
	pages = {142--148},
	pmid = {26585752},
	publisher = {Elsevier},
	title = {{Urinary metallomics as a novel biomarker discovery platform: Breast cancer as a case study}},
