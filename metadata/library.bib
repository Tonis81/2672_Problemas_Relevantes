@article{Panaullah2009,
	abstract = {Natural contamination of groundwater with arsenic (As) occurs around the$\backslash$nworld but is most widespread in the river basin deltas of South and$\backslash$nSoutheast Asia. Shallow groundwater is extensively used in the Bengal$\backslash$nbasin for irrigation of rice in the dry winter season, leading to the$\backslash$npossibility of As accumulation in soils, toxicity to rice and increased$\backslash$nlevels of As in rice grain and straw. The impact of As contaminated$\backslash$nirrigation water on soil-As content and rice productivity was studied$\backslash$nover two winter-season rice crops in the command area of a single$\backslash$ntubewell in Faridpur district, Bangladesh. After 16-17 years of use of$\backslash$nthe tubewell, a spatially variable build up of As and other chemical$\backslash$nconstituents of the water (Fe, Mn and P) was observed over the command$\backslash$narea, with soil-As levels ranging from about 10 to 70 mg kg(-1). A$\backslash$nsimple mass balance calculation using the current water As level of 0.13$\backslash$nmg As L(-1) suggested that 96{\%} of the added arsenic was retained in the$\backslash$nsoil. When BRRI dhan 29 rice was grown in two successive years across$\backslash$nthis soil-As gradient, yield declined progressively from 7-9 to 2-3 t$\backslash$nha(-1) with increasing soil-As concentration. The average yield loss$\backslash$nover the 8 ha command area was estimated to be 16{\%}. Rice-straw As$\backslash$ncontent increased with increasing soil-As concentration; however, the$\backslash$ntoxicity of As to rice resulted in reduced grain-As concentrations in$\backslash$none of the 2 years. The likelihood of As-induced yield reductions and As$\backslash$naccumulation in straw and grain has implications to agricultural$\backslash$nsustainability, food quality and food security in As-affected regions$\backslash$nthroughout South and Southeast Asia.},
	author = {Panaullah, Golam M. and Alam, Tariqul and Hossain, M. Baktear and Loeppert, Richard H. and Lauren, Julie G. and Meisner, Craig a. and Ahmed, Zia U. and Duxbury, John M.},
