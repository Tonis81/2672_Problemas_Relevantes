	issn = {13616579},
	journal = {Physiological Measurement},
	keywords = {arsenic,diagnostic test,inductively coupled plasma-mass spectrometry,x-ray fluorescence},
	number = {12},
	pages = {2443--2459},
	pmid = {26536141},
	publisher = {IOP Publishing},
	title = {{Evaluation of a novel portable x-ray fluorescence screening tool for detection of arsenic exposure}},
	url = {http://dx.doi.org/10.1088/0967-3334/36/12/2443},
	volume = {36},
