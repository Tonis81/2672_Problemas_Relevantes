	issn = {00139351},
	journal = {Environmental Research},
	keywords = {Biomonitoring,Cadmium,Child,Gender,Intelligence,Neuropsychological development,neuropsychological development},
	month = {oct},
	pages = {66--73},
	publisher = {Elsevier},
	title = {{Cadmium exposure and neuropsychological development in school children in southwestern Spain}},
