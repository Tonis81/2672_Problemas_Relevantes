@article{Huyck2007,
	abstract = {Objective: To characterize the effects of maternal arsenic exposure on birth weight. Methods: Hair, toenail, and drinking water samples were collected from pregnant women (n = 52) at multiple time points during pregnancy and from their newborns after birth. Total arsenic was measured using inductively coupled plasma-mass spectrometry. The association between arsenic and birth weight was investigated using linear and logistic regression models. Results: Maternal hair arsenic measured early in pregnancy was associated with decreased birth weight ($\beta$ = -193.5 $\pm$ 90.0 g, P = 0.04). Maternal hair and drinking water arsenic levels measured at first prenatal visit were significantly correlated with newborn hair arsenic level ($\rho$ = 0.32, P = 0.04 and $\rho$ = 0.31, P = 0.04). Conclusions: Results suggest that maternal arsenic exposure early in pregnancy negatively affects newborn birth weight and that maternal hair provides the best integrated measure of arsenic exposure. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
	author = {Huyck, Karen L. and Kile, Molly L. and Mahiuddin, Golam and Quamruzzaman, Quazi and Rahman, Mahmuder and Breton, Carrie V. and Dobson, Christine B. and Frelich, Janna and Hoffman, Elaine and Yousuf, Jabed and Afroz, Sakila and Islam, Shofiqul and Christiani, David C.},
