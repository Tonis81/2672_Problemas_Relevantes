	issn = {15354970},
	journal = {American Journal of Respiratory and Critical Care Medicine},
	keywords = {Life course,Lung function,Nitrogen oxide,Particulate matter,Spirometry},
	month = {jan},
	number = {2},
	pages = {171--177},
	publisher = {American Thoracic Society},
	title = {{Early-life exposure to traffic-related air pollution and lung function in adolescence}},
	url = {https://pubmed.ncbi.nlm.nih.gov/26397124/},
	volume = {193},
