	pages = {28--39},
	pmid = {22137101},
	publisher = {Elsevier},
	title = {{Humans seem to produce arsenobetaine and dimethylarsinate after a bolus dose of seafood}},
	url = {http://dx.doi.org/10.1016/j.envres.2011.11.007},
