	doi = {10.1016/j.scitotenv.2010.06.041},
	eprint = {NIHMS150003},
	file = {:Users/tonisignes/Desktop/Dropbox/TONI SIGNES/A{\_}BOOKS {\&} ARTICLES/ARTICLES MENDELEY pdf/1-s2.0-S0048969710006467-main.pdf:pdf},
