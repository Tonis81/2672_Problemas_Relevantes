	issn = {10960333},
	journal = {Toxicology and Applied Pharmacology},
	keywords = {1,3,Arsenic,Cardiovascular disease,Ischemic heart disease,Mortality,New Hampshire,Smoking,a global health issue,arsenic,arsenic exposure via contaminated,cardiovascular disease,in the us,ischemic heart disease,mortality,nearly 7,new hampshire,people rely on private,smoking,water and diet is,water source,wells as their primary,where over 15 million},
