	issn = {14698137},
	journal = {New Phytologist},
	keywords = {mineral nutrient,molybdenum (Mo),natural variation,quantitative trait locus (QTL),rice},
	month = {mar},
	number = {4},
	pages = {1983--1997},
	pmid = {30339276},
	publisher = {Blackwell Publishing Ltd},
	title = {{Natural variation in a molybdate transporter controls grain molybdenum concentration in rice}},
