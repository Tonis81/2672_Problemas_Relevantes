@article{Pettibone2018,
	abstract = {The National Institute of Environmental Health Sciences (NIEHS) introduces a new translational research framework that builds upon previous biomedical models to create a more comprehensive and integrated environmental health paradigm. The framework was developed as a graphical construct that illustrates the complexity of designing, implementing, and tracking translational research in environmental health. We conceptualize translational research as a series of concentric rings and nodes, defining ``translation'' as movement either from one ring to another or between nodes on a ring. A ``Fundamental Questions'' ring expands upon the research described in other frameworks as ``basic'' to include three interrelated concepts critical to basic science research: research questions, experimental settings, and organisms. This feature enables us to capture more granularity and thus facilitates an approach for categorizing translational research and its growth over time. We anticipate that the framework will help researchers develop compelling long-term translational research stories and accelerate public health impacts by clearly mapping out opportunities for collaborations. By using this paradigm, researchers everywhere will be better positioned to design research programs, identify research partners based on cross-disciplinary research needs, identify stakeholders who are likely to use the research for environmental decision-making and intervention, and track progress toward common goals.},
	author = {Pettibone, Kristianna G. and Balshaw, David M. and Dilworth, Caroline and Drew, Christina H. and Hall, Janet E. and Heacock, Michelle and Latoni, Alfonso R. and McAllister, Kimberly A. and O'fallon, Liam R. and Thompson, Claudia and Walker, Nigel J. and Wolfe, Mary S. and Wright, Demia S. and Collman, Gwen W.},
	doi = {10.1289/EHP3657},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Pettibone et al. - 2018 - Expanding the Concept of Translational Research Making a Place for Environmental Health Sciences.pdf:pdf},
	issn = {15529924},
	journal = {Environmental Health Perspectives},
	month = {jul},
	number = {7},
	pmid = {30024381},
	publisher = {National Institute of Environmental Health Sciences},
	title = {{Expanding the concept of translational research: Making a place for environmental health sciences}},
	url = {/pmc/articles/PMC6108854/ /pmc/articles/PMC6108854/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6108854/},
	volume = {126},
	year = {2018},
	bdsk-url-1 = {/pmc/articles/PMC6108854/%20/pmc/articles/PMC6108854/?report=abstract%20https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6108854/},
