	keywords = {Biomonitoring Equivalents,Inorganic arsenic,Pharmacokinetics,Risk assessment},
	number = {1},
	pages = {1--9},
	pmid = {20547195},
	publisher = {Elsevier Inc.},
	title = {{Biomonitoring Equivalents for inorganic arsenic}},
