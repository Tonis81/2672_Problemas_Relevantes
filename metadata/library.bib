	pmid = {33161204},
	publisher = {Elsevier Ltd},
	title = {{Maternal and childhood exposure to inorganic arsenic and airway allergy -- A 15-Year birth cohort follow-up study}},
	url = {https://doi.org/10.1016/j.envint.2020.106243},
	volume = {146},
	year = {2021},
