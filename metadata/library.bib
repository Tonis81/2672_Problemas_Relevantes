	issn = {01678809},
	journal = {Agriculture, Ecosystems and Environment},
	keywords = {Alternate wetting and drying,Arsenic concentration,Dietary risk assessment,Random forest,Rice grain,Vermicompost},
	number = {March},
	pages = {107516},
	publisher = {Elsevier B.V.},
	title = {{Deficit irrigation and organic amendments can reduce dietary arsenic risk from rice: Introducing machine learning-based prediction models from field data}},
	url = {https://doi.org/10.1016/j.agee.2021.107516},
	volume = {319},
