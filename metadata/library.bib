@article{Rahman2015,
	abstract = {Arsenic exposure is associated with cancer and vascular diseases. Angiogenesis is an important step for the pathological development of cancer and vascular diseases. Vascular endothelial growth factor (VEGF) is a specific marker for angiogenesis. However, human study showing the association between arsenic exposure and serum VEGF levels has not yet been documented. This study was aimed to investigate the association between arsenic exposure and serum VEGF levels in the arsenic-endemic individuals in Bangladesh. A total of 260 individuals were recruited for this study. Arsenic exposure levels were measured by ICP-MS and VEGF levels were quantified using VEGF immunoassay kit. The study subjects were stratified into tertile (low, medium and high) groups based on the arsenic in water, hair and nails. Serum VEGF levels were correlated with water (rs=0.363, p{\textless}0.001), hair (rs=0.205, p{\textless}0.01) and nail (rs=0.190, p{\textless}0.01) arsenic. Further, VEGF levels showed dose-response relationships with water, hair and nail arsenic. Mean VEGF levels in≤10$\mu$gL-1, 10.1-50 $\mu$gL-1and {\textgreater} 50$\mu$gL-1groups were 91.84, 129.54, and 169.86pgmL-1, respectively, however, significant (p{\textless}0.01) difference in VEGF levels was only found in {\textgreater} 50$\mu$gL-1versus ≤10$\mu$gL-1groups. Significant associations of arsenic exposure with VEGF levels were found even after adjusting with relevant covariates. Therefore, these results provide evidence that arsenic exposure has a pro-angiogenic effect on humans, which may be implicated in arsenic-induced tumorigenesis and vascular diseases.},
	author = {Rahman, Mashiur and Mamun, Abdullah Al and Karim, Md Rezaul and Islam, Khairul and Amin, Hasan Al and Hossain, Shakhawoat and Hossain, Md Imam and Saud, Zahangir Alam and Noman, Abu Shadat Mohammod and Miyataka, Hideki and Himeno, Seiichiro and Hossain, Khaled},
	doi = {10.1016/j.chemosphere.2014.08.003},
