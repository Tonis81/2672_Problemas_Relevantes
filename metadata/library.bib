	issn = {1559-0631},
	journal = {Journal of Exposure Science and Environmental Epidemiology},
	keywords = {manganese biomarker drinking water,toenails},
	number = {December},
	pages = {648--654},
	title = {{Toenail manganese as biomarker of drinking water exposure: a reliability study from a US pregnancy cohort}},
	url = {https://browzine.com/articles/255642379 https://doi.org/10.1038/s41370-018-0108-z},
