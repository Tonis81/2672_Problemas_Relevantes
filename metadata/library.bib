	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Schmitt et al. - 2005 - Human nails as a biomarker of arsenic exposure from well water in Inner Mongolia Comparing atomic flourescence s.pdf:pdf},
	issn = {1354750X},
	journal = {Biomarkers},
	keywords = {Analysis,Arsenic,Atomic flourescence spectrometry,Drinking water,Nails,Neutron activation},
	number = {2-3},
	pages = {95--104},
	pmid = {16076725},
	title = {{Human nails as a biomarker of arsenic exposure from well water in Inner Mongolia: Comparing atomic flourescence spectrometry and neutron activation analysis}},
	volume = {10},
	year = {2005},
