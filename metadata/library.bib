	keywords = {Intellectual quotient,Metals,Neurocognitive functions,Neurotoxicity,Schoolchildren,Social stressors},
	month = {jul},
	number = {1},
	pages = {67},
	pmid = {31324194},
	publisher = {BioMed Central Ltd.},
	title = {{Neurocognitive impact of metal exposure and social stressors among schoolchildren in Taranto, Italy}},
