	isbn = {1606-0997 (Print)$\backslash$r1606-0997 (Linking)},
	issn = {16060997},
	journal = {Journal of Health, Population and Nutrition},
	keywords = {Arsenic,Bottled water,Drinking-water,United States},
	number = {3},
