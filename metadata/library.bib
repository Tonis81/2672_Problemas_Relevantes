	keywords = {Arsenic,Grain,Shoot,Soil},
	number = {3},
	pages = {476--479},
	pmid = {18757098},
	publisher = {Elsevier Ltd},
	title = {{Enhanced transfer of arsenic to grain for Bangladesh grown rice compared to US and EU}},
	url = {http://dx.doi.org/10.1016/j.envint.2008.07.010},
	volume = {35},
