	title = {{Polymorphisms in nucleotide excision repair genes, arsenic exposure, and non-melanoma skin cancer in New Hampshire}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/17687452{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pmc/articles/PMC1940098/pdf/ehp0115-001231.pdf},
