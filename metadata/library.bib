@article{Haynes2015,
	abstract = {BACKGROUND: Manganese (Mn) plays a vital role in brain growth and development, yet excessive exposure can result in neurotoxicity. Marietta, Ohio, is home to the nation's longest-operating ferromanganese refinery, and community concern about exposure led to the development of the research study.$\backslash$n$\backslash$nOBJECTIVES: Our overall goal was to address the community's primary research question: "Does Mn affect cognitive development of children?" We evaluated the relationships between Mn exposure as measured by blood and hair Mn, along with other neurotoxicants including blood lead (Pb) and serum cotinine, and child cognition.$\backslash$n$\backslash$nMETHODS: Children 7-9 years of age were enrolled (n = 404) in the Communities Actively Researching Exposure Study (CARES) from Marietta and Cambridge, Ohio, and their surrounding communities from October 2008 through March 2013. Blood and hair were analyzed for Mn and Pb, and serum was analyzed for cotinine. We used penalized splines to assess potential nonlinear associations between biological measures and IQ subscale scores, followed by multivariable regression models with categorical variables based on quartiles of the distribution for biological measures with nonlinear associations and continuous variables for biological measures with linear associations.$\backslash$n$\backslash$nRESULTS: Geometric mean blood (n = 327) and hair Mn (n = 370) concentrations were 9.67 $\pm$ 1.27 $\mu$g/L and 416.51 $\pm$ 2.44 ng/g, respectively. After adjusting for potential confounders, both low and high blood and hair Mn concentrations were associated with lower Full Scale IQ and subscale scores, with significant negative associations between the highest quartile and middle two quartiles of blood Mn ($\beta$ -3.51; 95{\%} CI: -6.64, -0.38) and hair Mn ($\beta$ -3.66; 95{\%} CI: -6.9, -0.43{\%}) and Full Scale IQ.$\backslash$n$\backslash$nCONCLUSIONS: Both low and high Mn concentrations in blood and hair were negatively associated with child IQ scores. Serum cotinine was negatively associated with child cognitive function.$\backslash$n$\backslash$nCITATION: Haynes EN, Sucharew H, Kuhnell P, Alden J, Barnas M, Wright RO, Parsons PJ, Aldous KM, Praamsma ML, Beidler C, Dietrich KN. 2015. Manganese exposure and neurocognitive outcomes in rural school-age children: the Communities Actively Researching Exposure Study (Ohio, USA). Environ Health Perspect 123:1066-1071; http://dx.doi.org/10.1289/ehp.1408993.},
	author = {Haynes, Erin N. and Sucharew, Heidi and Kuhnell, Pierce and Alden, Jody and Barnas, Mary and Wright, Robert O. and Parsons, Patrick J. and Aldous, Kenneth M. and Praamsma, Meredith L. and Beidler, Caroline and Dietrich, Kim N.},
	doi = {10.1289/ehp.1408993},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Haynes et al. - 2015 - Manganese exposure and neurocognitive outcomes in rural school-age children The communities actively researching.pdf:pdf},
	isbn = {5135585427},
