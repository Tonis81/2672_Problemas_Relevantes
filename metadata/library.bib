	isbn = {1879-1298 (Electronic)$\backslash$r0045-6535 (Linking)},
	issn = {00456535},
	journal = {Chemosphere},
	keywords = {Arsenic,Fish,Inter-individual variability,Intra-individual variability,Seafood,Urinary speciation},
	number = {9},
	pages = {898--905},
	pmid = {22104336},
	publisher = {Elsevier Ltd},
	title = {{The influence of diet on intra and inter-individual variability of urinary excretion of arsenic species in Italian healthy individuals}},
	url = {http://dx.doi.org/10.1016/j.chemosphere.2011.10.050},
