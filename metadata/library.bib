	issn = {10960333},
	journal = {Toxicology and Applied Pharmacology},
	keywords = {8-Nitroguanine,DNA methylation,Inflammatory genes,Prenatal arsenic exposure},
	pages = {36--47},
	publisher = {Elsevier Inc.},
	title = {{Hypomethylation of inflammatory genes (COX2, EGR1, and SOCS3) and increased urinary 8-nitroguanine in arsenic-exposed newborns and children}},
