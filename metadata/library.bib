	isbn = {0022-3476},
	issn = {10976833},
	journal = {Journal of Pediatrics},
	number = {1},
	pages = {138--147.e3},
	pmid = {25818499},
	publisher = {Elsevier Inc},
	title = {{Associations between gut microbial colonization in early life and respiratory outcomes in cystic fibrosis}},
	url = {http://dx.doi.org/10.1016/j.jpeds.2015.02.049},
	volume = {167},
