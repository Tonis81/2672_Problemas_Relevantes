	arxivid = {NIHMS150003},
	author = {Lambrou, Angeliki and Baccarelli, Andrea and Wright, Robert O. and Weisskopf, Marc and Bollati, Valentina and Amarasiriwardena, Chitra and Vokonas, Pantel and Schwartza, Joel},
	doi = {10.1097/EDE.0b013e31825afb0b},
	eprint = {NIHMS150003},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Lambrou et al. - 2012 - Arsenic exposure and DNA methylation among elderly men.pdf:pdf},
