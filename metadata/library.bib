	isbn = {0300-483X (Print)$\backslash$r0300-483X (Linking)},
	issn = {0300483X},
	journal = {Toxicology},
	keywords = {Arsenic,Bangladesh,Blood biomarker,Case-cohort study,Skin lesions},
	number = {2-3},
	pages = {225--233},
	pmid = {16860454},
	title = {{Blood arsenic as a biomarker of arsenic exposure: Results from a prospective study}},
	volume = {225},
