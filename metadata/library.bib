	publisher = {Am J Respir Crit Care Med},
	title = {{Traffic-related air pollution and the development of asthma and allergies during the first 8 years of life}},
	url = {https://pubmed.ncbi.nlm.nih.gov/19965811/},
	volume = {181},
	year = {2010},
