	issn = {00070963},
	journal = {British Journal of Dermatology},
	number = {4},
	pages = {783--793},
	pmid = {23772556},
	title = {{Psoriasis, psoriatic arthritis and type 2 diabetes mellitus: A systematic review and meta-analysis}},
	volume = {169},
	year = {2013},
