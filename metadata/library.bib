@article{Shen2016,
	abstract = {Chronic arsenic exposure is a critical public health issue in many countries. The metabolism of arsenic in vivo is complicated because it can be influenced by many factors. In the present meta-analysis, two researchers independently searched electronic databases, including the Cochrane Library, PubMed, Springer, Embase, and China National Knowledge Infrastructure, to analyze factors influencing arsenic methylation. The concentrations of the following arsenic metabolites increase (p{\textless} 0.000001) following arsenic exposure: inorganic arsenic (iAs), monomethyl arsenic (MMA), dimethyl arsenic (DMA), and total arsenic. Additionally, the percentages of iAs (standard mean difference (SMD): 1.00; 95{\%} confidence interval (CI): 0.60-1.40; p{\textless} 0.00001) and MMA (SMD: 0.49; 95{\%} CI: 0.21-0.77; p = 0.0006) also increase, while the percentage of DMA (SMD: -0.57; 95{\%} CI: -0.80--0.31; p{\textless} 0.0001), primary methylation index (SMD: -0.57; 95{\%} CI: -0.94--0.20; p = 0.002), and secondary methylation index (SMD: -0.27; 95{\%} CI: -0.46--0.90; p = 0.004) decrease. Smoking, drinking, and older age can reduce arsenic methylation, and arsenic methylation is more efficient in women than in men. The results of this analysis may provide information regarding the role of arsenic oxidative methylation in the arsenic poisoning process.},
	author = {Shen, Hui and Niu, Qiang and Xu, Mengchuan and Rui, Dongsheng and Xu, Shangzhi and Feng, Gangling and Ding, Yusong and Li, Shugang and Jing, Mingxia},
	doi = {10.3390/ijerph13020205},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Shen et al. - 2016 - Factors affecting arsenic methylation in arsenic-exposed humans A systematic review and meta-analysis.pdf:pdf},
	isbn = {1824093322},
	issn = {16604601},
	journal = {International Journal of Environmental Research and Public Health},
	keywords = {Arsenic,Human,Meta-analysis,Methylation},
	number = {2},
	pmid = {26861378},
	title = {{Factors affecting arsenic methylation in arsenic-exposed humans: A systematic review and meta-analysis}},
	volume = {13},
	year = {2016},
	bdsk-url-1 = {https://doi.org/10.3390/ijerph13020205}}

