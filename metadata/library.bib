	issn = {02786915},
	journal = {Food and Chemical Toxicology},
	keywords = {Carry-over effect,Cooking,Diet,Endemic areas,Intake,Small and medium-sized enterprises},
	pages = {601--608},
	pmid = {24075917},
	title = {{Relationship between arsenic content of food and water applied for food processing}},
