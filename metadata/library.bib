	journal = {Environmental Science and Pollution Research},
	keywords = {Cancer mortality,Epidemiology,Geochem,Geochemistry,INLA,SPDE,Spatial data,article,cancer mortality,electronic supplementary material the,epidemiology,geochemistry,inla,online version of this,philippe garrigues,responsible editor,spatial data,spde},
	number = {17},
