	journal = {Analytical and Bioanalytical Chemistry},
	keywords = {Anion-exchange,HPLC-ICP-MS,Inorganic arsenic,Microwave-assisted alkaline dissolution,Seafood},
	number = {2},
	pages = {339--346},
	pmid = {15517207},
	title = {{Determination of inorganic arsenic in white fish using microwave-assisted alkaline alcoholic sample dissolution and HPLC-ICP-MS}},
