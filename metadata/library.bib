@article{Steinmaus2014,
	abstract = {BACKGROUND: From 1958 to 1970, {\textgreater}100,000 people in northern Chile were exposed to  a well-documented, distinct period of high drinking water arsenic concentrations. We previously reported ecological evidence suggesting that early-life exposure in this population resulted in increased mortality in adults from several outcomes, including lung and bladder cancer. METHODS: We have now completed the first study ever assessing incident cancer cases after early-life arsenic exposure, and the first study on this topic with individual participant exposure and confounding factor data. Subjects included 221 lung and 160 bladder cancer cases diagnosed in northern Chile from 2007 to 2010, and 508 age and gender-matched controls. RESULTS: ORs adjusted for age, sex, and smoking in those only exposed in early life to arsenic water concentrations of {\textless}/=110, 110 to 800, and {\textgreater}800 mug/L were 1.00, 1.88 [95{\%} confidence interval (CI), 0.96-3.71], and 5.24 (3.05-9.00; P(trend) {\textless} 0.001) for lung cancer, and 1.00, 2.94 (1.29-6.70), and 8.11 (4.31-15.25; P(trend) {\textless} 0.001) for bladder cancer. ORs were lower in those not exposed until adulthood. The highest category ({\textgreater}800 mug/L) involved exposures that started 49 to 52 years before, and ended 37 to 40 years before the cancer cases were diagnosed. CONCLUSION: Lung and bladder cancer incidence in adults was markedly increased following exposure to arsenic in early life, even up to 40 years after high exposures ceased. Such findings have not been identified before for any environmental exposure, and suggest that humans are extraordinarily susceptible to early-life arsenic exposure. IMPACT: Policies aimed at reducing early-life exposure may help reduce the long-term risks of arsenic-related disease.},
	author = {Steinmaus, Craig and Ferreccio, Catterina and Acevedo, Johanna and Yuan, Yan and Liaw, Jane and Dur{\'{a}}n, Viviana and Cuevas, Susana and Garc{\'{i}}a, Jos{\'{e}} and Meza, Rodrigo and Vald{\'{e}}s, Rodrigo and Vald{\'{e}}s, Gustavo and Ben{\'{i}}tez, Hugo and {Van Der Linde}, Vania and Villagra, Vania and Cantor, Kenneth P. and Moore, Lee E. and Perez, Saida G. and Steinmaus, Scott and Smith, Allan H.},
	doi = {10.1158/1055-9965.EPI-14-0059},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Steinmaus et al. - 2014 - Increased lung and bladder cancer incidence in adults after in utero and early-life arsenic exposure.pdf:pdf},
	issn = {10559965},
	journal = {Cancer Epidemiology Biomarkers and Prevention},
	number = {8},
	pages = {1529--1538},
	pmid = {24859871},
	title = {{Increased lung and bladder cancer incidence in adults after in utero and early-life arsenic exposure}},
	volume = {23},
	year = {2014},
	bdsk-url-1 = {https://doi.org/10.1158/1055-9965.EPI-14-0059}}

@article{J2019,
	abstract = {The purpose of this study was to examine if the short semi-quantitative food frequency questionnaire (FFQ) is a reliable and valid tool to assess the diet of Spanish children aged 7-9 years. We collected data from 156 children of the birth cohort INMA (Infancia y Medio Ambiente (Environment and Childhood)). Children's parents or care-givers completed a 46-item FFQ on two occasions over a 9-12-month period about the children's diet. To explore the reproducibility of the FFQ, the nutrient and food group intake collected from the both FFQs were compared, while validity was examined by contrasting the nutrient values from the FFQs and the average of three 24-hour dietary recalls (24hDRs) taken in this period, and also with the concentration of several vitamins in the blood (carotenoids, vitamin D and a-tocopherol). Pearson and de-attenuated correlation coefficients were calculated. The average correlation coefficients for nutrient intake's reproducibility was 0.41, ranging from 0.25 (calcium) to 0.65 (Β-carotene), and for food group intake was 0.45, ranging from 0.18 (cereals) to 0.68 (sweetened beverages). Correlation coefficients slightly improved when we compared energy-adjusted intakes. The average correlation coefficients for validity against 24hDRs was 0.34 for energy-adjusted intakes, and 0.39 when de-attenuation coefficients were used. The validity coefficients against the blood concentrations of vitamins were 0.38 for Β-cryptoxanthin, 0.26 for lycopene, 0,23 for a-carotene and 0.15 for Β-carotene, all of them statistically significant (p {\textless} 0.05). This study suggests that our brief FFQ is a suitable tool for the dietary assessment of a wide range of nutrients and food groups in children 7-9 years, despite the low to moderate reproducibility and validity observed for some nutrients.},
	author = {Vioque, Jesus and Garcia-De-La-Hera, Manuela and Gonzalez-Palacios, Sandra and Torres-Collado, Laura and Notario-Barandiaran, Leyre and Oncina-Canovas, Alejandro and Soler-Blasco, Raquel and Lozano, Manuel and Beneito, Andrea and Navarrete-Mu{\~{n}}oz, Eva Maria},
	doi = {10.3390/NU11040933},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/J et al. - 2019 - Reproducibility and Validity of a Short Food Frequency Questionnaire for Dietary Assessment in Children Aged 7⁻9 Yea.pdf:pdf},
	issn = {20726643},
	journal = {Nutrients},
	keywords = {24-hour dietary recall,Childhood,Diet,Food frequency questionnaire,Mediterranean country,Nutrient intake,Nutritional biomarker,Reproducibility,Validity},
	month = {apr},
	number = {4},
