@article{Cano-Lamadrid2020,
	abstract = {Anthropogenic agronomic practices could negatively affect the agricultural soil which is the main source for inorganic arsenic (i-As) contamination in rice fields due to flooding. The presence of essential elements could be impacted by food industry units operations. The aim of this study was to evaluate the effect of husking and polishing on mineral essential elements and inorganic As species in rice from Protected Natural Reserve ``Marjal de Pego-Oliva''. The amount of all studied elements was significantly affected by husking (a decrease of Ca, Na and Pb, 360 to 101 mg kg−1, 202 to 46.9 mg kg-1 and 17.9 to 5.70 $\mu$g kg−1, respectively) and by polishing (a reduction of Na, Mg, P, K, Ca and Cu, 46.9--26.8 mg kg−1, 1600 to 481 mg kg−1, 4050 to 1530 mg kg−1, 2780 to 882 mg kg−1, 101 to 53.7 mg kg−1, and 3600 to 2840, $\mu$g kg−1, respectively).The replacement of white rice (WR) to brown rice (BR) could enhance the daily dietary intake of essential elements to reach the dietary reference values and the maximum i-As (40.9 and 44.9 i-As $\mu$g kg-1 for BR and WR, respectively) was below the maximum level recently established by European Food Safety Agency.},
	author = {Cano-Lamadrid, Marina and Girona, D{\'{a}}maris and Garc{\'{i}}a-Garc{\'{i}}a, Elena and Dominguis-Rovira, Vicente and Domingo, Concha and Sendra, Esther and L{\'{o}}pez-Lluch, David and Carbonell-Barrachina, {\'{A}}ngel A.},
