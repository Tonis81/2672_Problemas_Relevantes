@article{Hall2007,
	abstract = {BACKGROUND: In Bangladesh, tens of millions of people have been consuming waterborne arsenic for decades. The extent to which As is transported to the fetus during pregnancy has not been well characterized.$\backslash$n$\backslash$nOBJECTIVES: We therefore conducted a study of 101 pregnant women who gave birth in Matlab, Bangladesh.$\backslash$n$\backslash$nMETHODS: Maternal and cord blood pairs were collected and concentrations of total As were analyzed for 101 pairs, and As metabolites for 30 pairs. Maternal urinary As metabolites and plasma folate, cobalamin, and homocysteine levels in maternal cord pairs were also measured. Household tube well-water As concentrations exceeded the World Health Organization guideline of 10 microg/L in 38{\%} of the cases.$\backslash$n$\backslash$nRESULTS: We observed strong associations between maternal and cord blood concentrations of total As (r = 0.93, p {\textless} 0.0001). Maternal and cord blood arsenic metabolites (n = 30) were also strongly correlated: in dimethylarsinate (DMA) (r = 0.94, p {\textless} 0.0001), monomethylarsonate (r = 0.80, p {\textless} 0.0001), arsenite (As(+3)) (r = 0.80, p {\textless} 0.0001), and arsenate (As(+5)) (r = 0.89, p {\textless} 0.0001). Maternal homocysteine was a strong predictor of {\%}DMA in maternal urine, maternal blood, and cord blood (beta = -6.2, p {\textless} 0.02; beta = -10.9, p {\textless} 0.04; and beta = -13.7, p {\textless} 0.04, respectively). Maternal folate was inversely associated with maternal blood As(5+) (beta = 0.56, p {\textless} 0.05), and maternal cobalamin was inversely associated with cord blood As(5+) (beta = -1.2, p {\textless} 0.01).$\backslash$n$\backslash$nCONCLUSIONS: We conclude that exposure to all metabolites of inorganic As occurs in the prenatal period.},
	author = {Hall, Marni and Gamble, Mary and Slavkovich, Vesna and Liu, Xinhua and Levy, Diane and Cheng, Zhongqi and van Geen, Alexander and Yunus, Mahammad and Rahman, Mahfuzar and Pilsner, J. Richard and Graziano, Joseph},
