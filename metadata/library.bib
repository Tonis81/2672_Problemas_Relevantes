	isbn = {1111111111},
	issn = {19326203},
	journal = {PLoS ONE},
	number = {7},
	pages = {1--13},
	pmid = {29985964},
	title = {{Effect of dexamethasone exposure on the neonatal unit on the school age lung function of children born very prematurely}},
	volume = {13},
	year = {2018},
