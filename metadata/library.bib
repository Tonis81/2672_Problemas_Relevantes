	keywords = {Adult,Ascorbic Acid,Ascorbic Acid: administration {\&} dosage,Ascorbic Acid: blood,Biomarkers,Biomarkers: blood,Carotenoids,Carotenoids: administration {\&} dosage,Carotenoids: blood,Cohort Studies,Diet,Energy Intake,Female,Folic Acid,Folic Acid: administration {\&} dosage,Folic Acid: blood,Humans,Mediterranean Region,Nutrition Assessment,Pregnancy,Prospective Studies,Reproducibility of Results,Surveys and Questionnaires,Vitamin B 12,Vitamin B 12: administration {\&} dosage,Vitamin B 12: blood,alpha-Tocopherol,alpha-Tocopherol: administration {\&} dosage,alpha-Tocopherol: blood,beta Carotene,beta Carotene: administration {\&} dosage,beta Carotene: blood},
