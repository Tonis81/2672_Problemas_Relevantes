	keywords = {Dietary fibre,Dry fractionation,Electrostatic separation,Oil binding capacity,Swelling capacity,Water retention capacity},
	pages = {50--57},
	publisher = {Elsevier Ltd},
	title = {{Dietary fibre enrichment from defatted rice bran by dry fractionation}},
