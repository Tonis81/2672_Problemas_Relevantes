	doi = {10.1016/j.envres.2016.07.003},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Murcia et al. - 2016 - Prenatal mercury exposure and birth outcomes.pdf:pdf},
	issn = {00139351},
	journal = {Environmental Research},
	keywords = {Birth weight,Diet, food, and nutrition,Gestational age,Maternal exposure,Mercury,Placentation},
	pages = {11--20},
	publisher = {Elsevier},
	title = {{Prenatal mercury exposure and birth outcomes}},
	url = {http://dx.doi.org/10.1016/j.envres.2016.07.003},
