	keywords = {Arab-Americans,Biomarkers,ICP-MS,Multielement analysis,Toenails},
	number = {2},
	pages = {113--126},
	pmid = {16217136},
	title = {{Profiles of trace elements in toenails of Arab-Americans in the Detroit Area, Michigan}},
