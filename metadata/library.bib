	bdsk-url-2 = {https://doi.org/10.1371/journal.pone.0120992}}

@article{EFSA2010,
	abstract = {Lead occurs primarily in the inorganic form in the environment. Human exposure is mainly via food and water, with some via air, dust and soil. In average adult consumers, lead dietary exposure ranges from 0.36 to 1.24, up to 2.43 µg/kg body weight (b.w.) per day in high consumers in Europe. Exposure of infants ranges from 0.21 to 0.94 µg/kg b.w. per day and in children from 0.80 to 3.10 (average consumers), up to 5.51 (high consumers) µg/kg b.w. per day. Cereal products contribute most to dietary lead exposure, while dust and soil can be important non-dietary sources in children. Lead is absorbed more in children than in adults and accumulates in soft tissues and, over time, in bones. Half-lives of lead in blood and bone are approximately 30 days and 10 30 years, respectively, and excretion is primarily in urine and faeces. The Panel on Contaminants in the Food Chain (CONTAM Panel) identified developmental neurotoxicity in young children and cardiovascular effects and nephrotoxicity in adults as the critical effects for the risk assessment. The respective BMDLs derived from blood lead levels in µg/L (corresponding dietary intake values in µg/kg b.w. per day) were: developmental neurotoxicity BMDL01, 12 (0.50); effects on systolic blood pressure BMDL01, 36 (1.50); effects on prevalence of chronic kidney disease BMDL10, 15 (0.63). The CONTAM Panel concluded that the current PTWI of 25 $\mu$g/kg b.w. is no longer appropriate as there is no evidence for a threshold for critical lead-induced effects. In adults, children and infants the margins of exposures were such that the possibility of an effect from lead in some consumers, particularly in children from 1--7 years of age, cannot be excluded. Protection of children against the potential risk of neurodevelopmental effects would be protective for all other adverse effects of lead, in all populations.},
	author = {EFSA},
	doi = {10.2903/j.efsa.2010.1570},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Unknown - 2010 - Scientific Opinion on Lead in Food.pdf:pdf;:Users/tonisignes/Dropbox (Personal)/TONI SIGNES/A{\_}BOOKS {\&} ARTICLES/ARTICLES MENDELEY pdf/EFSA2010.pdf:pdf},
	issn = {18314732},
	journal = {EFSA Journal},
	keywords = {adults,children,dietary exposure,food consumption,lead,margin of exposure,occurrence,risk assessment},
	month = {apr},
	number = {4},
	publisher = {Wiley-Blackwell Publishing Ltd},
	title = {{Scientific Opinion on Lead in Food}},
	volume = {8},
	year = {2010},
	bdsk-url-1 = {https://doi.org/10.2903/j.efsa.2010.1570}}

@article{Beydon2002,
	abstract = {Pulmonary function tests have rarely been assessed in preschool children with cystic fibrosis (CF). The objective of this multicenter study was to compare pulmonary function in 39 preschool children with CF (height, 90-130 cm; 16 homozygous $\Delta$F508) and in 79 healthy control children. Functional residual capacity (helium dilution technique) and expiratory interrupter resistance (Rint exp) (interrupter technique) were measured. As compared with control children, children with CF had significantly higher Rint exp, expressed as absolute values and as Z-scores (1.05 $\pm$ 0.36 versus 0.80 $\pm$ 0.20 kPa{\textperiodcentered}L -1{\textperiodcentered}second, p {\textless} 0.0001; and 1.31 $\pm$ 1.72 versus 0.19 $\pm$ 0.97, p {\textless} 0.0001), and significantly lower specific expiratory interrupter conductance (1.29 $\pm$ 0.34 versus 1.63 $\pm$ 0.43 kPa -1{\textperiodcentered}second, p {\textless} 0.0001). The effect of the bronchodilator salbutamol on Rint exp was not significantly different between children with CF and control children. Rint exp Z-scores were significantly higher in children with CF who were exposed to passive smoke (n = 8) (p {\textless} 0.03). Children with CF and with a history of respiratory symptoms (n = 31) had significantly higher functional residual capacity Z-scores (p {\textless} 0.02) and lower specific expiratory interrupter conductance Z-scores (p {\textless} 0.04). Genotype did not influence the data. We conclude that Rint exp and functional residual capacity measurements may help to follow young children with CF who are unable to perform reproducible forced expiratory maneuvers.},
	author = {Beydon, Nicole and Amsallem, Francis and Bellet, Mireille and Boul{\'{e}}, Mich{\`{e}}le and Chaussain, Mich{\`{e}}le and Denjean, Andr{\'{e}} and Matran, R{\'{e}}gis and Pin, Isabelle and Alberti, Corinne and Gaultier, Claude},
	doi = {10.1164/rccm.200205-421OC},
	file = {:Users/tonisignes/Desktop/Dropbox/TONI SIGNES/A{\_}BOOKS {\&} ARTICLES/ARTICLES MENDELEY pdf/Beydon2002.pdf:pdf},
	issn = {1073449X},
	journal = {American Journal of Respiratory and Critical Care Medicine},
	keywords = {Bronchodilator,Expiratory interrupter resistance,Functional residual capacity,Genotype,Passive smoke},
	number = {8},
	pages = {1099--1104},
	pmid = {12379554},
	title = {{Pulmonary function tests in preschool children with cystic fibrosis}},
	volume = {166},
	year = {2002},
	bdsk-url-1 = {https://doi.org/10.1164/rccm.200205-421OC}}

@article{Yusa2018a,
	abstract = {We present a new approach to arsenic (As) risk assessment using biomonitoring. In this pilot study we determined the levels of total and speciated urinary arsenicin 109 Spanish school children aged between 6 and 11 years, and interpreted these concentrations in a risk assessment context. The geometric mean (GM) for total As (TAs) was 33.82 $\mu$/L. The order of occurrence and average concentrations of the different species was arsenobetaine (AsB) (100{\%}, 15 $\mu$g/L), dimethylarsinic (DMA) (97{\%}, 8.32 $\mu$g/L), monomethylarsonic (MMA) (26{\%}, 0.27 $\mu$g/L) and inorganic As (iAs) (4{\%}, 0.14 $\mu$g/L). 18{\%} of children presented exposures to inorganic arsenic (7.52 $\mu$g/g creatinine) higher than guidance value for non-cancer risk (8.3 $\mu$g/g creatinine). For cancer risk the exposure to inorganic arsenic was much higher than the guidance value. Urinary DMA was positively associated with urinary AsB, suggesting exposure directly to this specie or metabolism of organic arsenicals to this specie, mainly through seafood consumption. Consequently, the exposure to inorganic As needs to be carefully interpreted because it may be overestimated. Our study supports the hypothesis that urinary iAs + MMA is the most reliable biomarker of exposure to inorganic As.},
	author = {Yus{\`{a}}, Vicent and P{\'{e}}rez, Rosa and S{\'{a}}nchez, Alfredo and Pardo, Olga and Roca, Marta},
	doi = {10.1016/j.scitotenv.2018.01.330},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Yus{\`{a}} et al. - 2018 - Exposure and risk assessment to arsenic species in Spanish children using biomonitoring.pdf:pdf;:Users/tonisignes/Desktop/Dropbox/TONI SIGNES/A{\_}BOOKS {\&} ARTICLES/ARTICLES MENDELEY pdf/Exposure and risk assessment to arsenic species in Spanish children using biomonitoring.pdf:pdf},
	issn = {18791026},
	journal = {Science of the Total Environment},
	pages = {302--309},
	publisher = {Elsevier B.V.},
	title = {{Exposure and risk assessment to arsenic species in Spanish children using biomonitoring}},
	url = {https://doi.org/10.1016/j.scitotenv.2018.01.330},
	volume = {628-629},
	year = {2018},
	bdsk-url-1 = {https://doi.org/10.1016/j.scitotenv.2018.01.330}}

@article{Raab2005,
	abstract = {Ingested arsenic is known to be not only excreted by urine, but to be stored in sulphydryl-rich tissue like hair, nail or skin. We developed an extraction method for arsenic species from these tissues and studied the stability of different arsenic species during the extraction process. Inorganic and pentavalent methylated arsenic was found to be stable under the extraction conditions, whereas trivalent methylated arsenicals and the thio-analogue of DMA(V) stop (DMAS) showed reduced stability. The absorption ability of hair for these different species was studied as well. Inorganic arsenic is better absorbed by hair than monomethyl- or dimethyl-arsenicals, whereby the trivalent forms are taken up better than the pentavalent forms. Independent of which methylated arsenical was used for the incubation, the pentavalent form was always the dominant form after extraction. Hair and nail samples from humans suffering from chronic arsenic intoxication contained dominantly inorganic arsenic with small and strongly varying amounts of DMA(V) stop and MA(V) stop present. DMAS was only found in some nail sample extracts containing unusually high amounts of DMA(V) stop and is believed to be formed during the extraction process.},
	author = {Raab, Andrea and Feldmann, J.},
	doi = {10.1007/s00216-004-2796-6},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Raab, Feldmann - 2005 - Arsenic speciation in hair extracts.pdf:pdf},
	isbn = {1618-2642},
	issn = {16182642},
	journal = {Analytical and Bioanalytical Chemistry},
	keywords = {Accumulation potential,Arsenic speciation,HPLC-ICP-MS,Hair analysis,Sample preparation,Species transformation},
	number = {2},
	pages = {332--338},
	pmid = {15340771},
	title = {{Arsenic speciation in hair extracts}},
	volume = {381},
	year = {2005},
	bdsk-url-1 = {https://doi.org/10.1007/s00216-004-2796-6}}

@article{ODonovan2015,
	abstract = {OBJECTIVE: To describe adherence with infant feeding and complementary feeding guidelines.$\backslash$n$\backslash$nDESIGN: Prospective study of infant feeding and complementary feeding practices were collected as part of the Cork BASELINE Birth Cohort Study.$\backslash$n$\backslash$nSETTING: Cork, Ireland.$\backslash$n$\backslash$nSUBJECTS: Data are described for the 823 infants for whom a diary was completed.$\backslash$n$\backslash$nRESULTS: Breast-feeding was initiated in 81 {\%} of infants, and 34 {\%}, 14 {\%} and 1 {\%} of infants were exclusively breast-fed at hospital discharge, 2 and 6 months, respectively. Stage one infant formula decreased from 71 {\%} at 2 months to 13 {\%} at 12 months. The majority of infants (79 {\%}) were introduced to solids between 17 and 26 weeks and 18 {\%} were given solid foods before 17 weeks. Mothers of infants who commenced complementary feeding prior to 17 weeks were younger (29{\textperiodcentered}8 v. 31{\textperiodcentered}5 years; P{\textless}0{\textperiodcentered}001) and more likely to smoke (18 v. 8 {\%}; P=0{\textperiodcentered}004). The first food was usually baby rice (69 {\%}), infant breakfast cereals (14 {\%}) or fruit/vegetables (14 {\%}). Meals were generally home-made (49 {\%}), cereal-based (35 {\%}), manufactured (10 {\%}), dairy (3 {\%}) and dessert-based (3 {\%}). The median gap between the first-second, second-third, third-fourth and fourth-fifth new foods was 4, 2, 2 and 2 d, respectively.$\backslash$n$\backslash$nCONCLUSIONS: We present the largest prospective cohort study to date on early infant feeding in Ireland. The rate of breast-feeding is low by international norms. Most mothers introduce complementary foods between 4 and 6 months with lengthy gaps between each new food/food product. There is a high prevalence of exposure to infant breakfast cereals, which are composite foods, among the first foods introduced.},
	author = {O'Donovan, Sin{\'{e}}ad M and Murray, Deirdre M and Hourihane, Jonathan O'B O'B and Kenny, Louise C and Irvine, Alan D and Kiely, Mairead and O'Donovan, Sin{\'{e}}ad M and Murray, Deirdre M and Hourihane, Jonathan O'B O'B and Kenny, Louise C and Irvine, Alan D and Kiely, Mairead},
	doi = {10.1017/S136898001500018X},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/O'Donovan et al. - 2015 - Adherence with early infant feeding and complementary feeding guidelines in the Cork BASELINE Birth Cohort Stu.pdf:pdf},
	isbn = {1368980015},
