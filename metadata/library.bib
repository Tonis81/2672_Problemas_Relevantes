	isbn = {1660-4601 (Electronic)$\backslash$r1660-4601 (Linking)},
	issn = {16604601},
	journal = {International Journal of Environmental Research and Public Health},
	keywords = {Arsenic,Cardiomycocytes,Drinking water,Low-density array,Real-time PCR},
	number = {6},
	pages = {2090--2108},
	pmid = {21776218},
	title = {{Altered gene expression by low-dose arsenic exposure in humans and cultured cardiomyocytes: Assessment by real-time PCR arrays}},
	volume = {8},
	year = {2011},
	bdsk-url-1 = {https://doi.org/10.3390/ijerph8062090}}

@article{Ma2016,
	abstract = {Arsenic contaminations have been evaluated in rice grains from Hunan Province, China. Forty-three locally grown rice samples were collected from 40 counties. Arsenic species including As(III), As(V), MMA and DMA were separated and determined by HPLC-ICP-MS method. The mean concentration of total arsenic in rice samples was 129.4 $\pm$ 49.2 $\mu$g/kg, lower than the Chinese maximum contaminant levels of inorganic arsenic in rice (200 $\mu$g/kg). The dominant species detected in rice samples was As(III), following DMA, As(V) and MMA. The Pearson's correlation analysis indicated significant positive relationships between As(III) and total arsenic (r = 0.939, p {\textless} 0.01), and DMA and total arsenic (r = 0.761, p {\textless} 0.01). However, the percentage of As(III) decreased with the total arsenic concentration in rice (r = -. 0.515, p {\textless} 0.01). Spatial distribution map of total arsenic concentration in rice samples from Hunan Province was obtained using kriging interpolation. High levels of total arsenic in rice grains were observed in south and east regions associated with mining activities and urbanization process. The cancer risk and hazard quotient were employed to estimate the potential human health risk. The results suggested great carcinogenic risk and high potential non-carcinogenic risk to people consuming local rice in Hunan Province.},
	author = {Ma, Li and Wang, Lin and Jia, Yuyu and Yang, Zhaoguang},
