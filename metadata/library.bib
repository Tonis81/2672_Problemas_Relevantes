	keywords = {DNA methylation,Epigenetics,LINE-1,One-carbon metabolism gene variants,Smoking,Trace elements},
	number = {6},
	pages = {650--656},
	title = {{Genetic and non-genetic predictors of LINE-1 methylation in leukocyte DNA}},
	url = {https://doi.org/10.1289/ehp.1206068},
