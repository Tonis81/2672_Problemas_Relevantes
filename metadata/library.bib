	journal = {The ISME journal},
	keywords = {1,4-alpha-Glucan Branching Enzyme,1,4-alpha-Glucan Branching Enzyme: genetics,Adult,Animals,Bacteria,Bacteria: classification,Bacteria: enzymology,Bacteria: genetics,Cattle,Chickens,Feces,Feces: microbiology,Gastrointestinal Tract,Gastrointestinal Tract: microbiology,Humans,Metagenomics,Middle Aged,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Swine},
