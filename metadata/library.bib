	journal = {Environment International},
	keywords = {Arsenic,Biomarker,Drinking water,Epidemiological study,Human health,Risk assessment},
	number = {1},
	pages = {150--171},
	pmid = {22208756},
	publisher = {Elsevier Ltd},
