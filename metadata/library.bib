	publisher = {Nature Publishing Group},
	title = {{Contribution of breast milk and formula to arsenic exposure during the first year of life in a US prospective cohort}},
	url = {http://www.nature.com/doifinder/10.1038/jes.2015.69},
	volume = {26},
