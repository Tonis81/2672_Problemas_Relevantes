	title = {{Lead toxicity: A review}},
	volume = {8},
	year = {2015},
	bdsk-url-1 = {https://doi.org/10.1515/intox-2015-0009}}

@article{Mazumdar2011,
	abstract = {BACKGROUND: Early life lead exposure might be a risk factor for neurocognitive impairment in adulthood.$\backslash$n$\backslash$nOBJECTIVES: We sought to assess the relationship between early life environmental lead exposure and intellectual function in adulthood. We also attempted to identify which time period blood-lead concentrations are most predictive of adult outcome.$\backslash$n$\backslash$nMETHODS: We recruited adults in the Boston area who had participated as newborns and young children in a prospective cohort study that examined the relationship between lead exposure and childhood intellectual function. IQ was measured using the Wechsler Abbreviated Scale of Intelligence (WASI). The association between lead concentrations and IQ scores was examined using linear regression.$\backslash$n$\backslash$nRESULTS: Forty-three adults participated in neuropsychological testing. Childhood blood-lead concentration (mean of the blood-lead concentrations at ages 4 and 10 years) had the strongest relationship with Full-Scale IQ ($\beta$ = -1.89 $\pm$ 0.70, p = 0.01). Full-scale IQ was also significantly related to blood-lead concentration at age 6 months ($\beta$ = -1.66 $\pm$ 0.75, p = 0.03), 4 years ($\beta$ = -0.90 $\pm$ 0.41, p = 0.03) and 10 years ($\beta$ = -1.95 $\pm$ 0.80, p = 0.02). Adjusting for maternal IQ altered the significance of the regression coefficient.$\backslash$n$\backslash$nCONCLUSIONS: Our study suggests that lead exposure in childhood predicts intellectual functioning in young adulthood. Our results also suggest that school-age lead exposure may represent a period of increased susceptibility. Given the small sample size, however, the potentially confounding effects of maternal IQ cannot be excluded and should be evaluated in a larger study.},
	author = {Mazumdar, Maitreyi and Bellinger, David C. and Gregas, Matthew and Abanilla, Kathleen and Bacic, Janine and Needleman, Herbert L.},
	doi = {10.1186/1476-069X-10-24},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Mazumdar et al. - 2011 - Low-level environmental lead exposure in childhood and adult intellectual function A follow-up study.pdf:pdf},
	isbn = {1476-069X},
	issn = {1476069X},
	journal = {Environmental Health: A Global Access Science Source},
	number = {1},
	pages = {24},
	pmid = {21450073},
	publisher = {BioMed Central Ltd},
	title = {{Low-level environmental lead exposure in childhood and adult intellectual function: A follow-up study}},
	url = {http://www.ehjournal.net/content/10/1/24},
	volume = {10},
	year = {2011},
	bdsk-url-1 = {http://www.ehjournal.net/content/10/1/24},
	bdsk-url-2 = {https://doi.org/10.1186/1476-069X-10-24}}

@article{Carbonell-Barrachina2003,
	abstract = {Spanish legislation sets a maximum level for total arsenic (As) in confectionery products at 0.1 microg g(-)(1). The U.S. Food and Drug Administration limitations for glycyrrhizic acid in hard and soft candies are 160 and 31 mg g(-)(1), respectively. Arsenic and glycyrrhizic acid were determined in 22 different confectionery products: 9 throat pearls, 4 hard candies, and 9 soft candies. Arsenic and glycyrrhizic acid were quantified by atomic absorption spectrometry with hydride generation and high-performance liquid chromatography, respectively. Levels of glycyrrhizic acid were always below the maximum limits established by the U.S. FDA; however, the As concentration in seven of nine throat pearls (0.55 +/- 0.15 microg g(-)(1)) were above the Spanish maximum limit. A clear empirical relationship between the arsenic and glycyrrhizic acid concentrations was observed (R (2) = 0.9357), implying that to avoid high levels of potentially toxic arsenic in licorice confections high-quality licorice extract should be used.},
