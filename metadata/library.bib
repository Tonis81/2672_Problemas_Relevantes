	keywords = {arsenic,cardiovascular disease,dose-response,low level,meta-analysis,systematic review},
	number = {23},
	pages = {1--11},
	pmid = {33276700},
	title = {{Corrections to: Positive association of cardiovascular disease (cvd) with chronic exposure to drinking water arsenic (as) at concentrations below the who provisional guideline value: A systematic review and meta-analysis (Int. J. environ. res. public heal}},
