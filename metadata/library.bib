	isbn = {6036539010},
	issn = {1940-6215},
	journal = {Cancer prevention research (Philadelphia, Pa.)},
	keywords = {Case-Control Studies,Female,Humans,Immunohistochemistry,Male,Middle Aged,Nails,Nails: chemistry,Risk Factors,Selenium,Selenium: analysis,Sex Factors,Smoking,Smoking: adverse effects,Tumor Suppressor Protein p53,Tumor Suppressor Protein p53: metabolism,Urinary Bladder Neoplasms,Urinary Bladder Neoplasms: epidemiology,Urinary Bladder Neoplasms: metabolism},
