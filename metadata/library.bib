	keywords = {arsenic,biological markers,biomarkers,prenatal exposure},
	month = {sep},
	number = {5},
	pages = {467--73},
	pmid = {24896769},
	publisher = {Nature Publishing Group},
	title = {{Infant toenails as a biomarker of in utero arsenic exposure.}},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4141012{\&}tool=pmcentrez{\&}rendertype=abstract},
	volume = {24},
