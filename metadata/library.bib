	journal = {American Journal of Epidemiology},
	keywords = {Arsenic,Bladder neoplasms,Cohort studies,Nails,Smoking},
	number = {9},
	pages = {853--859},
	pmid = {15496537},
	title = {{Arsenic concentrations in prediagnostic toenails and the risk of bladder cancer in a cohort study of male smokers}},
	volume = {160},
	year = {2004},
