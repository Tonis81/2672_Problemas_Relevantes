@article{Gruber2012,
	abstract = {Background: Dietary factors such as folate, vitamin B12, protein, and methionine are important for the excretion of arsenic via one-carbon metabolism in undernourished populations exposed to high levels of arsenic via drinking water. However, the effects of dietary factors on toenail arsenic concentrations in well-nourished populations exposed to relatively low levels of water arsenic are unknown. Methods: As part of a population-based case-control study of skin and bladder cancer from the USA, we evaluated relationships between consumption of dietary factors and arsenic concentrations in toenail clippings. Consumption of each dietary factor was determined from a validated food frequency questionnaire. We used general linear models to examine the associations between toenail arsenic and each dietary factor, taking into account potentially confounding effects. Results: As expected, we found an inverse association between ln-transformed toenail arsenic and consumption of vitamin B12 (excluding supplements) and animal protein. Unexpectedly, there were also inverse associations with numerous dietary lipids (e.g., total fat, total animal fat, total vegetable fat, total monounsaturated fat, total polyunsaturated fat, and total saturated fat). Finally, increased toenail arsenic concentrations were associated with increased consumption of long chain n-3 fatty acids. Conclusion: In a relatively well-nourished population exposed to relatively low levels of arsenic via water, consumption of certain dietary lipids may decrease toenail arsenic concentration, while long chain n-3 fatty acids may increase toenail arsenic concentration, possibly due to their association with arsenolipids in fish tissue. {\textcopyright} 2012 Gruber et al.; licensee BioMed Central Ltd.},
	author = {Gruber, Joann F. and Karagas, M. R. and Gilbert-Diamond, Diane and Bagley, Pamela J. and Zens, M. Scot and Sayarath, Vicki and Punshon, Tracy and Morris, J. Steven and Cottingham, Kathryn L.},
	doi = {10.1186/1475-2891-11-45},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Gruber et al. - 2012 - Associations between toenail arsenic concentration and dietary factors in a New Hampshire population.pdf:pdf},
	isbn = {1475289111},
	issn = {14752891},
	journal = {Nutrition Journal},
	keywords = {Arsenic,Biomarkers,Epidemiology,Fatty acids,Fish,Folate,Lipids,One-carbon metabolism},
	number = {1},
	pages = {1},
	pmid = {22747713},
	title = {{Associations between toenail arsenic concentration and dietary factors in a New Hampshire population}},
	volume = {11},
	year = {2012},
	bdsk-url-1 = {https://doi.org/10.1186/1475-2891-11-45}}

@article{Hasibuzzaman2017,
	abstract = {Chronic exposure to arsenic is associated with increased morbidity and$\backslash$nmortality from cardiovascular disease (CVD); however, plausible$\backslash$nbiomarker for early prediction and the underlying mechanism of$\backslash$narsenic-related CVD have not yet been clearly understood. Endothelial$\backslash$ndysfunction plays a central role in the development of CVD. We$\backslash$nhypothesized that endothelial damage or dysfunction is an important$\backslash$naspect and may be an early event of arsenic-related CVD. Soluble$\backslash$nthrombomodulin (sTM) in serum is thought to be a specific and stable$\backslash$nmarker for endothelial damage or dysfunction. This study was designed to$\backslash$nevaluate the association between chronic exposure to arsenic and sTM$\backslash$namong human subjects in arsenic- endemic and non-endemic rural areas in$\backslash$nBangladesh. A total of 321 study subjects (217 from arsenic-endemic$\backslash$nareas and 104 from a non-endemic area) were recruited. Subjects' arsenic$\backslash$nexposure levels (i.e., drinking water, hair and nail arsenic$\backslash$nconcentrations) were measured by Inductively Coupled Plasma Mass$\backslash$nSpectroscopy. The subjects' serum sTM levels were quantified by$\backslash$nimmunoassay kit. The average sTM levels of the subjects in$\backslash$narsenic-endemic and non-endemic areas were 4.58 +/- 2.20 and 2.84 +/-$\backslash$n1.29 (ng mL(-1)) respectively, and the difference was significant$\backslash$n(p{\textless}0.001). Arsenic exposure levels showed a significant (water arsenic:$\backslash$nrs = 0.339, p{\textless}0.001, hair arsenic: rs = 0.352, p{\textless}0.001 and nail arsenic:$\backslash$nrs = 0.308, p{\textless}0.001) positive associations with sTM levels. Soluble TM$\backslash$nlevels were higher in the higher exposure gradients if we stratified the$\backslash$nsubjects into tertile groups (low, medium and high) based on the arsenic$\backslash$nconcentrations of the subjects' drinking water, hair and nails. Finally,$\backslash$nincreased levels of sTM were negatively correlated with high density$\backslash$nlipoprotein cholesterol (HDL-C), and positively correlated with$\backslash$nintercellular adhesion molecule-1 (ICAM-1) and vascular cell adhesion$\backslash$nmolecule-1 (VCAM-1). Results of this study show that chronic exposure to$\backslash$narsenic has mild to moderate association with sTM levels.},
	author = {Hasibuzzaman, M. M. and Hossain, Shakhawoat and Islam, Shofikul and Rahman, Atiqur and Anjum, Adiba and Hossain, Faruk and Mohanto, Nayan Chandra and Karim, Rezaul and Hoque, Mominul and Saud, Zahangir Alam and Miyataka, Hideki and Himeno, Seiichiro and Hossain, Khaled},
