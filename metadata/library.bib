	isbn = {0028-646X},
	issn = {0013-936X},
	journal = {Environmental science {\&} technology},
	keywords = {Arsenic,Arsenic: analysis,Chromatography,High Pressure Liquid,Mass Spectrometry,Mass Spectrometry: methods,Oryza sativa,Oryza sativa: chemistry,X-Rays},
	month = {feb},
	number = {4},
	pages = {1051--1057},
