	isbn = {1476-6256 (Electronic)$\backslash$r0002-9262 (Linking)},
	issn = {00029262},
	journal = {American Journal of Epidemiology},
	keywords = {Antioxidants,Arrhythmias, cardiac,Arsenic,Calcium channel blockers,Cardiovascular diseases,Environmental health,Long QT syndrome},
	number = {6},
	pages = {739--746},
	pmid = {19700500},
	title = {{Association between low-level environmental arsenic exposure and qt interval duration in a general population study}},
