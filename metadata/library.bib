	keywords = {Angiogenesis,Arsenic,Bangladesh,Cancer,Vascular endothelial growth factor},
	pages = {336--342},
	pmid = {25180936},
	publisher = {Elsevier Ltd},
	title = {{Associations of total arsenic in drinking water, hair and nails with serum vascular endothelial growth factor in arsenic-endemic individuals in Bangladesh}},
