	issn = {10934529},
	journal = {Journal of Environmental Science and Health - Part A Toxic/Hazardous Substances and Environmental Engineering},
	keywords = {Arsenic,Bronchiectasis,COPD,Chronic bronchitis,ILD,Lung disease,Lung function tests,West Bengal},
