	issn = {0269-7491},
	journal = {Environmental Pollution},
	keywords = {Arsenic,Detoxification,Soil,Transporters,Xylem loading},
	pages = {117389},
	title = {{Arsenic behavior in soil-plant system and its detoxification mechanisms in plants: A review}},
	url = {https://www.sciencedirect.com/science/article/pii/S0269749121009714},
	volume = {286},
	year = {2021},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0269749121009714},
